<mvc:View
    controllerName="matix.com.rep.customerlicense.zcustomerlicense.controller.View1"
    height="100%"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:f="sap.f"
    xmlns:fb="sap.ui.comp.filterbar"
    xmlns:svm="sap.ui.comp.smartvariants"
    xmlns:plugins="sap.m.plugins"
    xmlns:columnmenu="sap.m.table.columnmenu"
    xmlns:sap.ui.fl="sap.ui.fl"
    xmlns:sap.ui.dt="sap.ui.dt"
    xmlns:vm="sap.ui.fl.variants"
    xmlns:dnd="sap.ui.core.dnd"
    xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
    xmlns:u="sap.ui.unified"
    xmlns:form="sap.ui.layout.form"
    xmlns:uxap="sap.uxap"
>
    <f:DynamicPage
        id="dynamicPageId"
        preserveHeaderStateOnScroll="true"
        headerExpanded="{/headerExpanded}"
        showFooter="true"
        fitContent="true"
    >
        <f:header>
            <f:DynamicPageHeader>
                <form:SimpleForm
                    layout="ResponsiveGridLayout"
                    columnsL="2"
                    title="Stock Transfer Order"
                >
                    <form:content>
                        <core:Title />
                        <Label text="From Date" />
                        <DatePicker
                            placeholder="MMM dd, yyyy"
                            width="15rem"
                            value="{path:'oModelForFilters>/FromDate', type: 'sap.ui.model.type.Date', formatOptions: {UTC: true}}"
                        />
                        <Label text="Customer" />
                        <Input
                            width="15rem"
                            value="{path:'oModelForFilters>/FromCustmer'}"
                        />
                        <Label text="Region" />
                        <Input
                            width="15rem"
                            value="{path:'oModelForFilters>/FromRegion'}"
                        />
                        <core:Title />
                        <Label text="To Date" />
                        <DatePicker
                            placeholder="MMM dd, yyyy"
                            width="15rem"
                            value="{path:'oModelForFilters>/ToDate', type: 'sap.ui.model.type.Date', formatOptions: {UTC: true}}"
                        />
                        <Label />
                        <Input
                            width="15rem"
                            value="{path:'oModelForFilters>/ToCustmer'}"
                        />
                        <Label />
                        <Input
                            width="15rem"
                            value="{path:'oModelForFilters>/ToRegion'}"
                        />
                    </form:content>
                </form:SimpleForm>

                <Toolbar>
                    <Button
                        text="Search"
                        type="Emphasized"
                        press="onSearch"
                    />
                    <Button
                        text="Reset"
                        type="Reject"
                        press="onFilterClear"
                    />
                    <ToolbarSpacer />
                </Toolbar>
            </f:DynamicPageHeader>
        </f:header>
        <f:content>
            <Table
                id="SOTable"
                items="{oModelForTable>/}"
                ariaLabelledBy="title"
                fixedLayout="Strict"
                autoPopinMode="true"
                growing="true"
                sticky="ColumnHeaders,HeaderToolbar"
                popinLayout="GridSmall"
                delete="onItemsTableDelete"
                backgroundDesign="Solid"
                alternateRowColors="true"
            >
                <headerToolbar>
                    <OverflowToolbar>
                        <Title
                            id="title"
                            text="List of Customer License"
                        />
                        <ToolbarSpacer />
                        <!-- <Button icon="sap-icon://action-settings" press="openPersoDialog" tooltip="Settings" visible="{myProperties>/view}"/> -->
                    </OverflowToolbar>
                </headerToolbar>
                <dependents>
                    <plugins:ColumnResizer columnResize="onColumnResize" />
                    <columnmenu:Menu
                        id="menu"
                        beforeOpen="beforeOpenColumnMenu"
                    >
                        <columnmenu:QuickSort change="onSort">
                            <columnmenu:items>
                                <columnmenu:QuickSortItem />
                            </columnmenu:items>
                        </columnmenu:QuickSort>
                        <columnmenu:QuickGroup change="onGroup">
                            <columnmenu:items>
                                <columnmenu:QuickGroupItem />
                            </columnmenu:items>
                        </columnmenu:QuickGroup>
                        <columnmenu:items>
                            <columnmenu:ActionItem
                                icon="sap-icon://sort"
                                label="Sort"
                                press="onColumnHeaderItemPress"
                            />
                            <columnmenu:ActionItem
                                icon="sap-icon://group-2"
                                label="Group"
                                press="onColumnHeaderItemPress"
                            />
                            <columnmenu:ActionItem
                                icon="sap-icon://filter"
                                label="Filter"
                                press="onColumnHeaderItemPress"
                            />
                            <columnmenu:ActionItem
                                icon="sap-icon://table-column"
                                label="Columns"
                                press="onColumnHeaderItemPress"
                            />
                        </columnmenu:items>
                    </columnmenu:Menu>
                </dependents>
                <dragDropConfig>
                    <dnd:DragDropInfo
                        sourceAggregation="columns"
                        targetAggregation="columns"
                        dropPosition="Between"
                        drop="onColumnMove"
                    />
                </dragDropConfig>
                <noData>
                    <IllustratedMessage />
                </noData>
                <columns>
                    <Column headerMenu="menu">
                        <header>
                            <Text text="Dealer Number" />
                        </header>
                    </Column>
                    <Column headerMenu="menu">
                        <header>
                            <Text text="Dealer" />
                        </header>
                    </Column>
                    <Column
                        app:p13nKey="HsnCode"
                        headerMenu="menu"
                    >
                        <Text text="State Name" />
                    </Column>
                    <Column
                        app:p13nKey="SionNo"
                        headerMenu="menu"
                    >
                        <Text text="District" />
                    </Column>
                    <Column
                        app:p13nKey="Plant"
                        headerMenu="menu"
                    >
                        <Text text="FRC No" />
                    </Column>
                    <Column
                        app:p13nKey="Plant"
                        headerMenu="menu"
                    >
                        <Text text="Valid From" />
                    </Column>
                    <Column
                        app:p13nKey="Plant"
                        headerMenu="menu"
                    >
                        <Text text="Valid To" />
                    </Column>
                </columns>
                <items>
                    <ColumnListItem>
                        <Text text="{MaterialNumber}" />
                        <Text />
                        <Text />
                        <Text />
                        <Text />
                        <Text />
                        <Text />
                    </ColumnListItem>
                </items>
            </Table>
        </f:content>
    </f:DynamicPage>
</mvc:View>
